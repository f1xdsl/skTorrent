{
    "editor.tabSize": 4,
    "editor.rulers": [
        120
    ],
    "files.watcherExclude": {
        "**/build/**": true,
        "**/.git/**": true,
        "**/node_modules/**": true,
        "**/*.o": true,
        "**/*.obj": true
    },
    "search.exclude": {
        "**/build/**": true,
        "**/.git/**": true
    },
    "editor.renderWhitespace": "trailing",
    "editor.suggestSelection": "first",
    "editor.cursorSmoothCaretAnimation": "on",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.copyRelativePathSeparator": "/",
    "files.autoSave": "onFocusChange",
    "task.saveBeforeRun": "always",
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "always",
    "markdown.validate.enabled": true,
    "remote.WSL.fileWatcher.polling": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.vcFormat.indent.preserveComments": true,
    "C_Cpp.vcFormat.indent.namespaceContents": false,
    "C_Cpp.vcFormat.indent.caseContentsWhenBlock": true,
    "C_Cpp.vcFormat.space.pointerReferenceAlignment": "right",
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": false,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.formatting": "clangFormat",
    "cmake.configureOnOpen": false,
    "cmake.autoSelectActiveFolder": false,
    "cmake.configureOnEdit": false,
    "cmake.generator": "Ninja",
    "cmake.preferredGenerators": [
        "Ninja"
    ],
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.configureArgs": [
        "-DCMAKE_PREFIX_PATH=/home/user/Qt/6.5.3/gcc_64",
        "-DCMAKE_EXE_LINKER_FLAGS=-fuse-ld=mold",
        "-DCMAKE_MODULE_LINKER_FLAGS=-fuse-ld=mold",
        "-DCMAKE_SHARED_LINKER_FLAGS=-fuse-ld=mold",
    ],
    "cmake.options.statusBarVisibility": "visible",
    "cmake.environment": {
        "CXX": "g++",
        "CC": "gcc"
    },
    "C_Cpp.codeAnalysis.clangTidy.enabled": false,
    "C_Cpp.configurationWarnings": "disabled",
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.clang_format_style": "file",
    "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor",
        "*.svg": "default"
    },
    "testExplorer.useNativeTesting": false,
    "testMate.cpp.test.executables": "${workspaceFolder}/build/Test/*Test",
    "testMate.cpp.log.logfile": "${workspaceFolder}/.vscode/testmate.log",
    "testMate.cpp.log.logSucceedingTests": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false,
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120
    },
    "[yaml]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120
    },
    "[plaintext]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120
    },
    "[toml]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 120
    },
    "[c]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[cpp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[cuda-cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[cmake]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "cheshirekow.cmake-format"
    },
    "files.associations": {
        ".clang*": "yaml",
        "*.hpp.in": "cpp",
        "*.in": "cpp",
        "*.ipp": "cpp",
        "*.tcc": "cpp",
        "queue": "cpp",
